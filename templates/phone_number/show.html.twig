{% extends 'base.html.twig' %}

{% block title %}PhoneNumber{% endblock %}

{% block body %}
    <h1>{{ 'site.phone_numbers'|trans }}</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>{{ 'Type'|trans }}</th>
                <td>{{ phone_number.type }}</td>
            </tr>
            <tr>
                <th>{{ 'site.number'|trans }}</th>
                <td>{{ phone_number.number }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('phone_number_index') }}">{{ 'site.back_to_list'|trans }}</a>

    <a href="{{ path('phone_number_edit', {'id': phone_number.id}) }}">{{ 'site.edit'|trans }}</a>

    {{ include('phone_number/_delete_form.html.twig') }}
{% endblock %}
